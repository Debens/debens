{
    "name": "@debens/mobile",
    "version": "0.0.2-alpha.0",
    "files": [
        "ios/release/assets",
        "ios/release/main.jsbundle",
        "android/app/build/generated/assets"
    ],
    "scripts": {
        "install:pods": "cd ios ; arch -x86_64 bundle exec pod install",
        "version": "react-native-version --skip-tag",
        "start": "react-native webpack-start",
        "ios": "yarn ios:development",
        "ios:development": "react-native run-ios --scheme Development",
        "ios:staging": "react-native run-ios --scheme Staging",
        "ios:production": "react-native run-ios --scheme Production",
        "ios:release": "bundle exec fastlane ios beta",
        "android": "yarn android:development",
        "android:development": "react-native run-android --variant developmentDebug",
        "android:staging": "react-native run-android --variant stagingDebug",
        "android:production": "react-native run-android --variant productionDebug",
        "android:release": "bundle exec fastlane android beta",
        "bundle:ios": "yarn bundle:base --platform ios --bundle-output ios/main.jsbundle",
        "bundle:android": "yarn bundle:base --platform android --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.bundle.map --assets-dest android/app/src/main/res/",
        "bundle:base": "react-native webpack-bundle --entry-file index.ts",
        "test": "jest --passWithNoTests",
        "lint": "eslint .",
        "lint:fix": "yarn lint --fix",
        "lint:staged": "lint-staged"
    },
    "dependencies": {
        "@debens/graphql": "0.0.2-alpha.0",
        "@debens/mobile-atoms": "0.0.2-alpha.0",
        "@debens/mobile-messaging": "0.0.1",
        "@debens/mobile-molecules": "0.0.1",
        "@debens/mobile-organisms": "0.0.1",
        "@debens/mobile-onboarding": "0.0.2-alpha.0",
        "@debens/react-native-fido": "0.0.2-alpha.0",
        "@debens/theme": "0.0.2-alpha.0",
        "@react-native-async-storage/async-storage": "^1.17.11",
        "@react-navigation/native": "^6.0.6",
        "@react-navigation/native-stack": "^6.2.5",
        "@svgr/webpack": "^6.2.1",
        "luxon": "^1.22.0",
        "react": "18.1.0",
        "react-native": "0.70.1",
        "react-native-encrypted-storage": "^4.0.3",
        "react-native-codegen": "^0.69.1",
        "react-native-config": "^1.4.6",
        "react-native-gesture-handler": "^2.8.0",
        "react-native-get-random-values": "^1.8.0",
        "react-native-haptic-feedback": "^1.14.0",
        "react-native-reanimated": "3.0.0-rc.3",
        "react-native-safe-area-context": "^4.2.5",
        "react-native-screens": "^3.13.1",
        "react-native-svg": "^12.3.0",
        "react-native-svg-transformer": "^0.20.0",
        "react-redux": "^8.0.5",
        "redux": "^4.2.0",
        "redux-dynamic-modules": "^5.2.3",
        "redux-dynamic-modules-saga": "^5.2.3",
        "redux-saga": "^1.2.2"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/runtime": "^7.17.9",
        "@callstack/repack": "^3.0.1",
        "@debens/config-typescript": "0.0.2-alpha.0",
        "@debens/eslint-config": "0.0.2-alpha.0",
        "@react-native-community/eslint-config": "^3.0.1",
        "@types/jest": "^27.4.1",
        "@types/luxon": "^1.21.0",
        "@types/react": "^18.0.8",
        "@types/react-native": "^0.70.0",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^27.5.1",
        "babel-loader": "^8.2.5",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^8.14.0",
        "jest": "^27.5.1",
        "lint-staged": "^12.4.1",
        "metro-react-native-babel-preset": "^0.72.1",
        "react-native-version": "^4.0.0",
        "react-test-renderer": "18.1.0",
        "terser-webpack-plugin": "^5.3.1",
        "tslib": "^2.4.0",
        "typescript": "^4.6.3",
        "webpack": "^5.72.1"
    },
    "jest": {
        "preset": "react-native"
    }
}
