{
    "name": "@debens/service-identity",
    "version": "0.0.1",
    "description": "identity service",
    "license": "MIT",
    "files": [
        "build"
    ],
    "scripts": {
        "start": "node build/bundle",
        "develop": "concurrently \"yarn develop:server\" \"yarn develop:code-gen\"",
        "develop:server": "nodemon",
        "develop:code-gen": "nodemon -x tsoa spec-and-routes --watch src/api/controllers/*",
        "build": "npm-run-all build:*",
        "build:routes": "tsoa routes",
        "build:specs": "tsoa spec",
        "build:typescript": "tsc -b",
        "build:webpack": "webpack --mode production",
        "compile": "yarn build --noEmit",
        "clean": "yarn build --clean",
        "test": "jest --config jest.config.js",
        "test:coverage": "yarn test --coverage",
        "test:ci": "yarn test:coverage --ci --maxWorkers 1",
        "lint": "eslint src",
        "lint:fix": "yarn lint --fix",
        "lint:staged": "lint-staged"
    },
    "dependencies": {
        "@teamhanko/hanko-node": "^1.0.0",
        "@debens/http": "^0.0.1",
        "@tsoa/runtime": "^4.1.0",
        "body-parser": "^1.20.0",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-winston": "^4.2.0",
        "nodemon": "^2.0.16",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.4.0",
        "ts-node": "^10.8.1",
        "tsoa": "^4.1.0",
        "tsyringe": "^4.7.0",
        "uuid": "^8.3.2",
        "winston": "^3.7.2"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@debens/config-typescript": "^0.0.1",
        "@debens/eslint-config": "^0.0.1",
        "@debens/utils": "^0.0.1",
        "@types/jest": "^27.4.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "babel-jest": "^28.0.3",
        "concurrently": "^7.2.1",
        "copy-webpack-plugin": "^11.0.0",
        "ignore-loader": "^0.1.2",
        "jest": "^28.0.3",
        "lint-staged": "^12.4.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "ts-loader": "^9.3.1",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "tslib": "^2.4.0",
        "typescript": "^4.6.3",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    }
}
