{
    "name": "@debens/service-identity",
    "version": "0.0.2-alpha.0",
    "description": "identity service",
    "license": "MIT",
    "main": "build/src/index.js",
    "files": [
        "build"
    ],
    "scripts": {
        "prepublishOnly": "docker build --build-arg SCOPE=@debens/service-identity -f ../../node.dockerfile ../../ -t service-identity",
        "publish": "npm-run-all publish:*",
        "publish:tag": "docker tag service-identity debens/service-identity",
        "publish:push": "docker push debens/service-identity",
        "launch": "node build/src/main.js",
        "start": "nest start",
        "start:watch": "yarn start --watch",
        "build": "nest build",
        "compile": "yarn build --noEmit",
        "clean": "yarn build --clean",
        "test": "jest --passWithNoTests",
        "test:coverage": "yarn test --coverage",
        "test:ci": "yarn test:coverage --ci --maxWorkers 1",
        "lint": "eslint src",
        "lint:fix": "yarn lint --fix",
        "lint:staged": "lint-staged"
    },
    "dependencies": {
        "@debens/event-sourcing": "0.0.2-alpha.0",
        "@debens/http": "0.0.2-alpha.0",
        "@debens/nestjs-auth": "0.0.1",
        "@nestjs/cli": "^9.1.5",
        "@nestjs/core": "^9.2.0",
        "@nestjs/cqrs": "^9.0.1",
        "@nestjs/microservices": "^9.2.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.1.3",
        "@teamhanko/hanko-node": "^1.0.0",
        "amqp-connection-manager": "^4.1.9",
        "amqplib": "^0.10.3",
        "autobind-decorator": "^2.4.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cookie-parser": "^1.4.6",
        "jwks-rsa": "^3.0.1",
        "luxon": "^1.22.0",
        "morgan": "^1.10.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.7",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@debens/config-typescript": "0.0.2-alpha.0",
        "@debens/eslint-config": "0.0.2-alpha.0",
        "@debens/utils": "0.0.2-alpha.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/amqplib": "^0.8.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/jest": "^27.4.1",
        "@types/luxon": "^1.21.0",
        "@types/morgan": "^1.9.3",
        "@types/passport-jwt": "^3.0.7",
        "@types/passport-local": "^1.0.34",
        "@types/uuid": "^8.3.4",
        "babel-jest": "^28.0.3",
        "jest": "^28.0.3",
        "lint-staged": "^12.4.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "tslib": "^2.4.0",
        "typescript": "^4.6.3"
    }
}
